{
  "name": "security-token",
  "version": "0.2.0",
  "description": "micobo security token",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test",
    "gas_report": "truffle test --network gas_report",
    "deploy": "truffle migrate --reset",
    "compile": "solc --bin --optimize --optimize-runs 99999 -o build/solc --overwrite --allow-paths . contracts/token/SecurityToken.sol",
    "compile-all": "truffle compile --all"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/micoboteam/security-token.git"
  },
